{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "noUncheckedIndexedAccess": true,
    "rewriteRelativeImportExtensions": true,
    "strict": true,
    "target": "ESNext",
    "verbatimModuleSyntax": true
  }
}
